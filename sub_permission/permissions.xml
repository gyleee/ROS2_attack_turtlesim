<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.omg.org/spec/DDS-SECURITY/20170901/omg_shared_ca_permissions.xsd">
  <permissions>
    <grant name="/turtlesim/turtlesim_subscriber">
      <subject_name>CN=/turtlesim/turtlesim_subscriber</subject_name>
      <validity>
        <not_before>2025-02-01T23:50:22</not_before>
        <not_after>2035-01-31T23:50:22</not_after>
      </validity>
      <allow_rule>
        <domains>
          <id>30</id>
        </domains>
        <publish>
          <topics>
            <topic>rt/rosout</topic>
            <topic>rt/parameter_events</topic>
            <topic>*/turtlesim_subscriber/*</topic>
          </topics>
        </publish>
        <subscribe>
          <topics>
            <topic>rt/turtle1/pose</topic>
            <topic>rt/rosout</topic>
            <topic>rt/parameter_events</topic>
            <topic>*/turtlesim_subscriber/*</topic>
          </topics>
        </subscribe>
      </allow_rule>
      <allow_rule>
        <domains>
          <id>30</id>
        </domains>
        <publish>
          <topics>
            <topic>ros_discovery_info</topic>
          </topics>
        </publish>
        <subscribe>
          <topics>
            <topic>ros_discovery_info</topic>
          </topics>
        </subscribe>
      </allow_rule>
      <default>DENY</default>
    </grant>
  </permissions>
</dds>
